{
    "name": "@syncreeper/node-passthrough",
    "version": "0.1.0",
    "description": "Reverse SSH tunnel client for SyncReeper - connects home machine to VPS",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "bin": {
        "syncreeper-passthrough": "src/index.ts"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "tsx src/index.ts",
        "passthrough": "tsx src/index.ts",
        "clean": "rimraf dist"
    },
    "dependencies": {
        "@inquirer/prompts": "^7.0.0",
        "@syncreeper/shared": "workspace:*",
        "execa": "^9.0.0",
        "yargs": "^17.0.0"
    },
    "devDependencies": {
        "@types/node": "^20.0.0",
        "@types/yargs": "^17.0.0",
        "tsx": "^4.7.0",
        "typescript": "^5.0.0"
    }
}
